{
    "name": "devdojo/laravel-reactions",
    "type": "library",
    "description": "",
    "keywords": [
        "laravel",
        "package",
	"reactions",
	"like",
	"love",
	"eloquent",
	"trait"
    ],
    "homepage": "https://github.com/thedevdojo/laravel-reactions",
    "license": "MIT",
    "authors": [
        {
            "name": "Tony Lea",
            "email": "tony@devdojo.com",
            "homepage": "https://devdojo.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^8.1|^8.2",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.35",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.5.0"
    },
    "autoload": {
        "psr-4": {
            "DevDojo\\LaravelReactions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevDojo\\LaravelReactions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
